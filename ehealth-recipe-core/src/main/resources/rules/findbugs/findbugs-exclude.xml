<?xml version="1.0"?>
<FindBugsFilter>
    <!--更多详细的可参考网上的类似findbugs-exclude.xml的文件-->
    <!-- 包下java文件过滤 -->
    <Match>
        <Package name="~recipe\.prescription\..*" />
    </Match>
    <Match>
        <Package name="~recipe\.standalone\.*" />
    </Match>
    <Match>
        <Package name="~recipe\.util\.*" />
    </Match>
    <Match>
        <Package name="~recipe\.bean\.PatientRecipeBean" />
    </Match>


    <!--类方法过滤-->
    <Match>
        <Class name="recipe.constant.RecipeBussConstant"/>
        <Bug pattern="MS_SHOULD_BE_FINAL" />
    </Match>
    <Match>
        <Class name="recipe.drugsenterprise.CommonRemoteService"/>
        <Bug pattern="WMI_WRONG_MAP_ITERATOR" />
    </Match>
    <Match>
        <Class name="recipe.util.LocalStringUtil"/>
        <Bug pattern="WMI_WRONG_MAP_ITERATOR" />
    </Match>
    <Match>
        <Class name="recipe.Server" />
        <Bug pattern="DLS_DEAD_LOCAL_STORE" />
    </Match>
    <Match>
        <Class name="recipe.service.HosPrescriptionService" />
        <Bug pattern="RCN_REDUNDANT_NULLCHECK_OF_NONNULL_VALUE" />
    </Match>
    <Match>
        <Class name="recipe.service.DrugsEnterpriseTestService" />
        <Bug pattern="OS_OPEN_STREAM" />
    </Match>
    <Match>
        <Class name="recipe.bean.PatientRecipeBean" />
    </Match>
    <Match>
        <Bug pattern="DM_DEFAULT_ENCODING"/>
    </Match>
    <Match>
        <Bug pattern="WMI_WRONG_MAP_ITERATOR"/>
    </Match>

</FindBugsFilter>